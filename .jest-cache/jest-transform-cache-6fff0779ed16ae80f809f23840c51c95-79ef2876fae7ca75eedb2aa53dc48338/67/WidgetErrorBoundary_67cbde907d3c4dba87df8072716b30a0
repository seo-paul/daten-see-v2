            className: "bg-yellow-1bg-[#FDF9F3]bg-[#FDF9F3] text-yellow-8bg-[#FDF9F3]bg-[#FDF9F3] px-3 py-1 rounded text-sm hover:bg-yellow-2bg-[#FDF9F3]bg-[#FDF9F3] focus:outline-none focus:ring-2 focus:ring-yellow-5bg-[#FDF9F3]bg-[#FDF9F3]",
            children: "Widget neu laden"
          }),
          /* istanbul ignore next */
          (cov_1il1r54mc0().b[6][0]++, enableMinimizeOnError) &&
          /* istanbul ignore next */
          (cov_1il1r54mc0().b[6][1]++, /*#__PURE__*/(0, _jsxruntime.jsx)("button", {
            onClick: () => {
              /* istanbul ignore next */
              cov_1il1r54mc0().f[4]++;
              cov_1il1r54mc0().s[12]++;
              // This would trigger widget minimization
              // In a real implementation, this would call a prop function
              if (process.env.NODE_ENV === 'development') {
                /* istanbul ignore next */
                cov_1il1r54mc0().b[7][0]++;
                cov_1il1r54mc0().s[13]++;
                // eslint-disable-next-line no-console
                console.log('Minimizing widget:', widgetId);
              } else
              /* istanbul ignore next */
              {
                cov_1il1r54mc0().b[7][1]++;
              }
            },
            className: "bg-gray-1bg-[#FDF9F3]bg-[#FDF9F3] text-gray-8bg-[#FDF9F3]bg-[#FDF9F3] px-3 py-1 rounded text-sm hover:bg-gray-2bg-[#FDF9F3]bg-[#FDF9F3] focus:outline-none focus:ring-2 focus:ring-gray-5bg-[#FDF9F3]bg-[#FDF9F3]",
            children: "Widget ausblenden"
          }))]
        }),
        /* istanbul ignore next */
        (cov_1il1r54mc0().b[8][0]++, process.env.NODE_ENV === 'development') &&
        /* istanbul ignore next */
        (cov_1il1r54mc0().b[8][1]++, widgetId) &&
        /* istanbul ignore next */
        (cov_1il1r54mc0().b[8][2]++, /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
          className: "mt-3 text-xs text-gray-5bg-[#FDF9F3]bg-[#FDF9F3]",
          children: ["Widget ID: ", widgetId, " | Type: ", widgetType]
        }))]
      })]
    })
  })));
  /* istanbul ignore next */
  cov_1il1r54mc0().s[14]++;
  return /*#__PURE__*/(0, _jsxruntime.jsx)(_ErrorBoundary.ErrorBoundary, {
    level: "widget",
    fallback: customFallback,
    context: {
      widgetId,
      widgetType,
      title,
      enableMinimizeOnError,
      ...context
    },
    ...config,
    children: children
  });
}
