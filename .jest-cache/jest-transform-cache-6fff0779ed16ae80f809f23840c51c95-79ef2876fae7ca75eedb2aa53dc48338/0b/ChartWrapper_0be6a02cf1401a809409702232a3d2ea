      bodyColor: _config.CHART_COLORS.textSecondary,
      borderColor: _config.CHART_COLORS.border,
      borderWidth: 1,
      cornerRadius: 8,
      padding: 12
    }
  },
  scales: {
    x: {
      grid: {
        display: true,
        color: _config.CHART_COLORS.borderLight
